<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <title>LockYourStay | @ViewBag.Title</title>
    <link href="@Url.Content("~/favicon.ico")" rel="icon" />

    <link href='@Url.Content("~/Content/font-awesome.css")' rel="stylesheet">
    <link href='@Url.Content("~/Content/simple-line-icons.css")' rel="stylesheet">
    <link href='@Url.Content("~/Content/fullscreen-slider.css")' rel="stylesheet">
    <link href='@Url.Content("~/Content/bootstrap.css")' rel="stylesheet">
    <link href='@Url.Content("~/Content/app.css")' rel="stylesheet">
    <link href='@Url.Content("~/Content/lockyourstay.css")' rel="stylesheet" />
    <link href='@Url.Content("~/Content/jquery-ui.css")' rel="stylesheet" />
    <link href='@Url.Content("~/Content/Custom/Search/Search.css")' rel="stylesheet" />
    <!-- Web Fonts -->
    <link rel='stylesheet' type='text/css' href='//fonts.googleapis.com/css?family=Open+Sans:400,300,600&amp;subset=cyrillic,latin'>

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="notransition no-hidden">


    <!-- Header -->
    @Html.Partial("_Header")

    <!-- Content -->
    @RenderBody()

    <!-- Footer -->
    @Html.Partial("_Footer")


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <!--custom scripts for childs-->
    @RenderSection("scripts", false);
</body>
</html>
